{
  "applications": {
    "gecko": {
      "id": "jid0-ePBKtqqSeez8ThB1NInNoY7B0xc@jetpack"
    }
  },
  "background": {
    "scripts": ["data/redirect-to-dxr.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://mxr.mozilla.org/*/source/*",
        "https://mxr.mozilla.org/*/search*",
        "https://mxr.mozilla.org/*/ident*",
        "https://mxr.mozilla.org/*/find*"
      ],
      "js": ["data/contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "description": "Redirect MXR to DXR",
  "icons": {
    "64": "icon64.png"
  },
  "manifest_version": 2,
  "name": "Better MXR",
  "permissions": [
    "https://mxr.mozilla.org/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "0.4"
}
